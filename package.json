{
  "name": "service-hands",
  "version": "1.0.0",
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "csscomb": "^4.2.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^4.0.0-beta.1",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.4",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack --mode=production",
    "dev:build": "./node_modules/.bin/webpack --mode=development",
    "dev:server": "./node_modules/.bin/webpack-dev-server --mode=development",
    "eslint": "./node_modules/.bin/eslint ./",
    "eslint:fix": "./node_modules/.bin/eslint ./ --fix",
    "csscomb": "./node_modules/.bin/csscomb src",
    "prettier": "./node_modules/.bin/prettier"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run eslint:fix",
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "npm run csscomb",
      "git add"
    ]
  }
}
